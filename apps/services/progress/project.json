{
  "name": "services-progress",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/services/progress/src",
  "projectType": "application",
  "targets": {
    "codegen": {
      "command": "graphql-codegen --config apps/services/progress/codegen.ts"
    },
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "dependsOn": ["codegen"],
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/services/progress",
        "main": "apps/services/progress/src/main.ts",
        "tsConfig": "apps/services/progress/tsconfig.app.json",
        "isolatedConfig": true,
        "webpackConfig": "apps/services/progress/webpack.config.js",
        "assets": [
          {
            "glob": "*.graphql",
            "input": "apps/services/progress/src",
            "output": "."
          }
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "services-progress:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "services-progress:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/services/progress/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/services/progress/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
